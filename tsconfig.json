{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "module": "commonjs",
        "lib": ["es2017", "dom", "dom.iterable"],
        "target": "es5",
        "jsx": "react",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "baseUrl": "types",
        "typeRoots": ["types"],
        "downlevelIteration": true
    },
    "include": ["./src/**/*"],
    "exclude": ["node_modules"]
}
